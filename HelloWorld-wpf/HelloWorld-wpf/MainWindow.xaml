<Window x:Class="HelloWorld_wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HelloWorld_wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock Name="msg" Text="Message" VerticalAlignment="Top" Margin="20,20,20,20" FontSize="40"></TextBlock>
        <Button Content="hello world" Height="50" Width="107" VerticalAlignment="Center" HorizontalAlignment="Center" Click="Button_Click" IsEnabled="True" FontFamily="Segoe UI Emoji"/>
    </Grid>
</Window>
